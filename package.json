{
  "name": "rich-editor",
  "version": "0.1.8",
  "description": "A react editor component based on draftjs.",
  "main": "lib/index.js",
  "files": [
    "dist/",
    "lib/"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --copy-files --extensions .js,.jsx --out-dir lib",
    "start": "webpack-dev-server --config webpack.local.js --host 127.0.0.1 --port 8080",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "npm run build && webpack --config webpack.prod.js --devtool source-map",
    "prepublish": "npm run prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imshengli/rich-editor.git"
  },
  "keywords": [
    "editor",
    "rich",
    "editor",
    "markdown"
  ],
  "author": "zhangshengli@imshengli.com",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "draft-js": "^0.10.0",
    "draft-js-export-html": "^0.5.2",
    "draft-js-import-html": "^0.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jshint": "^2.9.4",
    "jslint": "^0.10.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "bugs": {
    "url": "https://github.com/imshengli/rich-editor/issues"
  },
  "homepage": "https://github.com/imshengli/rich-editor#readme"
}
